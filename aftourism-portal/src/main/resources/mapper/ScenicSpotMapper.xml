<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aftourism.portal.mapper.ScenicSpotMapper">

    <select id="selectAll" resultType="com.aftourism.portal.pojo.ScenicSpot">
        SELECT *
        FROM t_scenic_spot
        WHERE is_deleted = 0
        ORDER BY sort DESC, id DESC
    </select>
</mapper>
