<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aftourism.monitor.mapper.SystemMetricMapper">

    <select id="selectLatest" resultType="com.aftourism.monitor.pojo.SystemMetric">
        SELECT *
        FROM t_system_metric
        WHERE is_deleted = 0
        ORDER BY create_time DESC
        LIMIT 100
    </select>
</mapper>
